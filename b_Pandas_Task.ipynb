   "source": [
    "# 아래의 dict를 DataFrame으로 변환한다.\n",
    "# 전체 가격을 20%씩 인상한다.\n",
    "import pandas as pd\n",
    "fruit = {\n",
    "    \"Name\": [\"사과\", \"딸기\", \"감\", \"참외\"],\n",
    "    \"Price\": [2000, 8000, 13000, 6000]\n",
    "}\n",
    "\n",
    "\n",
    "df = pd.DataFrame(fruit)\n",
    "\n",
    "df['Price'] = df['Price'] * 1.2\n",
    "\n",
    "display(df)\n"
